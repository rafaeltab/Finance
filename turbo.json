{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": [
				"build/**"
			],
			"dependsOn": [
				"^build",
				"generate:sdk"
			]
		},
		"build:dev": {
			"outputs": [
				"build/**"
			],
			"dependsOn": [
				"^build:dev",
				"generate:sdk-dev"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start:dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": [
				"^build:dev"
			]
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"outputMode": "new-only",
			"dependsOn": [
				"^build"
			]
		},
		"clean": {
			"cache": false
		},
		"generate:sdk": {
			"outputs": [
				"lib/**"
			],
			"dependsOn": [
				"copy:swagger"
			]
		},
		"copy:swagger": {
			"outputs": [
				"swagger.json"
			],
			"dependsOn": [
				"^generate:swagger"
			]
		},
		"generate:swagger": {
			"outputs": [
				"swagger.json"
			],
			"dependsOn": [
				"build"
			],
			"cache": true
		},
		"generate:sdk-dev": {
			"outputs": [
				"lib/**"
			],
			"dependsOn": [
				"copy:swagger-dev"
			]
		},
		"copy:swagger-dev": {
			"outputs": [
				"swagger.json"
			],
			"dependsOn": [
				"^generate:swagger-dev"
			]
		},
		"generate:swagger-dev": {
			"outputs": [
				"swagger.json"
			],
			"dependsOn": [
				"build:dev"
			]
		},
		"lint": {
			"cache": true,
			"dependsOn": [
				"^build"
			]
		},
		"lint:fix": {
			"cache": false,
			"dependsOn": [
				"^build"
			]
		}
	}	
}