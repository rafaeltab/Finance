{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": [
				"build/**"
			],
			"dependsOn": [
				"^build"
			]
		},
		"build:dev": {
			"outputs": [
				"build/**"
			],
			"dependsOn": [
				"^build:dev"
			]
		},
		"dev": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"test": {
			"outputMode": "new-only",
			"dependsOn": [
				"^build"
			]
		},
		"clean": {
			"cache": false
		}
	}	
}