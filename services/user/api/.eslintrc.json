{
	"root": true,
	"extends": [
		"@finance/eslint-config-lib-lint/configs/base.cjs"
	],
	"parserOptions": {
		"project": "./tsconfig.eslint.json"
	},
	"overrides": [
		{
			"files": [
				"*.response.ts",
				"*.responses.ts",
				"*.body.ts"
			],
			"rules": {
				"max-classes-per-file": "off"
			}
		}
	]
}